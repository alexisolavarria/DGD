- content_for :sidebar do
  = render :partial => "admin/shared/sidebar"

#description-admin
  #group
    .title
      %h1 Pages

  .pages-table
    %table#admin-table.zebra-striped
      %tr
        %th.grey{:width => "50px"} Execs
        %th.grey{:width => "50px"} Desc.
        %th.grey{:width => "80px"} Contact info
        %th.grey{:width => "50px"} Events
        %th.blue Group
        %th.green Page
      = render :partial => "description", :collection => @descriptions